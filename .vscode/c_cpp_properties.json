{
    "configurations": [
        {
            "browse": {
                "databaseFilename": "${default}",
                "limitSymbolsToIncludedHeaders": false
            },
            "includePath": [
                "/opt/ros/noetic/include/**",
                "/usr/include/**",
                "/usr/include/eigen3",
                "/usr/include/pcl-1.10",
                "${workspaceFolder}/src/ndt_omp/include/**",
                "${workspaceFolder}/src/sentry_simulation/include/**",
                "${workspaceFolder}/src/sentry_navigation/include/**",
                "${workspaceFolder}/src/sentry_localization/include/**",
                "${workspaceFolder}/devel/include/**",
                "${workspaceFolder}/src/localization/include"
            ],
            "name": "ROS",
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu11",
            "cppStandard": "c++14"
        }
    ],
    "version": 4
}